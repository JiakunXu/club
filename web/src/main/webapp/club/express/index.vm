						<div class="page-content">
							 <!-- Slider -->
							<div class="swiper-container swiper-init" data-pagination=".swiper-pagination-h">
								<div class="swiper-wrapper">
									<div class="swiper-slide">
                                        <img src="$!{env.imgUrl}/image/express/ad-1.png" width="100%"></img>
									</div>
								</div>
								<!-- div class="swiper-pagination swiper-pagination-h"></div -->
							</div>
							
							<div class="" style="background-color:#ffffff;">
								<ul class="mui-flex" style="padding: 0; margin-bottom: 0;">
									<li class="cell" style="">
										<a class="component type3" href="$!{env.appUrl}/express/ship.htm" style="padding:4px" 0px="" 0px;="">
                                            <img class="" src="$!{env.imgUrl}/image/express/jj.png" width="36" height="36"></img>寄件
										</a>
									</li>
									<li class="cell" style="">
										<a class="component type3" href="$!{env.appUrl}/express/waybill.htm" style="padding:4px" 0px="" 0px;="">
                                            <img class="" src="$!{env.imgUrl}/image/express/ydcx.png" width="36" height="36"></img>运单查询
										</a>
									</li>
									<li class="cell" style="">
                                        <a class="component type3" href="$!{env.appUrl}/express/rate.htm" style="padding:4px" 0px="" 0px;="">
                                            <img class="" src="$!{env.imgUrl}/image/express/yfcx.png" width="36" height="36"></img>运费查询
                                        </a>
									</li>
								</ul>
								<ul class="mui-flex" style="padding: 0; margin-top: 0;">
									<li class="cell" style="">
                                        <a class="component type3" href="$!{env.appUrl}/express/time.htm" style="padding:4px" 0px="" 0px;="">
                                            <img class="" src="$!{env.imgUrl}/image/express/sxcx.png" width="36" height="36"></img>时效查询
                                        </a>
									</li>
									<li class="cell" style="">
                                        <a class="component type3" href="$!{env.appUrl}/express/store.htm" style="padding:4px" 0px="" 0px;="">
                                            <img class="" src="$!{env.imgUrl}/image/express/fwwd.png" width="36" height="36"></img>服务网点
                                        </a>
									</li>
									<li class="cell" style="">
                                        <a class="component type3" href="$!{env.appUrl}/express/range.htm" style="padding:4px" 0px="" 0px;="">
