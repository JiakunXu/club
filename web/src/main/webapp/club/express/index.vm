		#UA()
	</head>
	<body>
		<!-- Views -->
		<div class="views tabs toolbar-through">
			<!-- Your main view, should have "view-main" class -->
			<div id="view-1" class="view view-main tab active" data-page="index-1">
				<!-- Top Navbar-->
				<div class="navbar">
					<div class="navbar-inner">
