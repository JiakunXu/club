<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />
		<title>自定义菜单</title>
		#ICON()
        #CSSM(["weui"])
		#UA()
	</head>
	<body>
        <div class="weui_cells_title">自定义菜单</div>
        <div class="weui_cells weui_cells_access">
            <a class="weui_cell" href="javascript:goto('create');" >
                <span class="weui_cell_hd"><img src="" class="icon_nav" alt=""></span>
                <div class="weui_cell_bd weui_cell_primary">
                    <p>创建应用菜单</p>
                </div>
                <div class="weui_cell_ft"></div>
            </a>
            <a class="weui_cell" href="javascript:goto('delete');" >
                <span class="weui_cell_hd"><img src="" class="icon_nav" alt=""></span>
                <div class="weui_cell_bd weui_cell_primary">
                    <p>删除菜单</p>
                </div>
                <div class="weui_cell_ft"></div>
            </a>
            <a class="weui_cell" href="javascript:goto('get');" >
                <span class="weui_cell_hd"><img src="" class="icon_nav" alt=""></span>
                <div class="weui_cell_bd weui_cell_primary">
                    <p>获取菜单列表</p>
                </div>
                <div class="weui_cell_ft"></div>
            </a>
        </div>
		
		#JS_ENV_VAL()
		<script type="text/javascript">
		function goto(op) {
			top.location.href = appUrl + "/weixin/menu.htm?op=" + op;
		}
		</script>
    </body>
</html>